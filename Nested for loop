	<script type="text/javascript">
		for (i=0; i<10; i++) {
			document.write("Naimul" +"<br>");
			for(j=0; j<10; j++){
			document.write("Naimul 2"+"<br>");	
			}
		}
		document.write("Code End");
	</script>
